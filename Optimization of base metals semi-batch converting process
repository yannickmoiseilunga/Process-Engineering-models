$if not set gp_count $set gp_count 0
$eval       gp_count %gp_count%+1
$log        gp_count=%gp_count%
$setglobal  gp_count %gp_count%

*-----------------------------------------------------------------------------
*       Declarations:
*-----------------------------------------------------------------------------

$if not declared gp__1          alias(gp__1,gp__2,*);
$if not declared gp_row         set gp_row(*), gp_col(*);
